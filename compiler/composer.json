{
    "name": "rector/rector-compiler",
    "description": "PHAR Compiler for Rector",
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "nette/neon": "^3.0",
        "nette/utils": "^3.1",
        "ondram/ci-detector": "^3.4",
        "sebastian/diff": "^3.0|^4.0",
        "symfony/console": "^4.4|^5.0",
        "symfony/process": "^4.4|^5.0",
        "symfony/filesystem": "^4.4|^5.0",
        "symfony/finder": "^4.4|^5.0",
        "symplify/composer-json-manipulator": "dev-master",
        "symplify/console-color-diff": "dev-master",
        "symplify/auto-bind-parameter": "dev-master",
        "symplify/smart-file-system": "dev-master",
        "symplify/easy-testing": "dev-master",
        "symplify/package-builder": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Rector\\Compiler\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rector\\Compiler\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
